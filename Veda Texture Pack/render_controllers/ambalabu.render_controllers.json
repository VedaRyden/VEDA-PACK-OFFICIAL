{
  "format_version":"1.16.0",
  "render_controllers":{
    "controller.render.ambalabu_pov":{
      "geometry":"Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "part_visibility":[{"muka":"q.swell_amount"}],
      "textures":["Texture.default"]
    },
    "controller.render.ambalabu_ptp":{
      "geometry":"Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "part_visibility":[{"muka":"q.swell_amount"}],
      "textures":["Texture.default"]
    },
    "controller.render.ambalabu_muka_pov":{
      "geometry":"Geometry.muka",
      "materials": [ { "*": "Material.muka" } ],
      "textures": [
        "q.time_of_day>0.75 ? Texture.malam",
        "q.time_of_day>0 >= 0.25 ? Texture.malam",
        "q.time_of_day>0.25 >= 0.75 ? Texture.muka"
      ]
    },
    "controller.render.ambalabu_muka_ptp":{
      "geometry":"Geometry.muka",
      "materials": [ { "*": "Material.muka" } ],
      "textures":["Texture.muka"]
    }
  }
}
